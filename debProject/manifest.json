{
    "manifest_version": 3,
    "name": "Breaking News",
    "description": "Get the latest breaking news from top news websites",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}